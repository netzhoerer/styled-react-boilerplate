{
    "presets": [
        [
            "@babel/preset-env",
            {
                "targets": {
                    "esmodules": true
                },
                "useBuiltIns": "usage",
                "corejs": 3
            }
        ],
        "@babel/preset-react"
    ],
    "plugins": [
        [
            "babel-plugin-styled-components",
            {
                "minify": true,
                "pure": true
            }
        ],
        "react-css-modules",
        "@babel/plugin-proposal-export-default-from",
        "react-hot-loader/babel"
    ]
}
